# DataFusion Service环境变量示例
# 复制此文件为.env并根据需要修改

# 配置文件路径
CONFIG_PATH=config.toml

# 服务器配置
DATAFUSION_HOST=0.0.0.0
DATAFUSION_PORT=50051
DATAFUSION_MAX_CONNECTIONS=100

# 查询配置
DATAFUSION_MAX_MEMORY_MB=4096
DATAFUSION_BATCH_SIZE=8192
DATAFUSION_TIMEOUT_SECONDS=300

# TLS配置
DATAFUSION_TLS_ENABLED=false
# DATAFUSION_TLS_CERT=/path/to/cert.pem
# DATAFUSION_TLS_KEY=/path/to/key.pem

# 日志配置
DATAFUSION_LOG_LEVEL=info
DATAFUSION_LOG_FORMAT=json

# Rust日志配置
RUST_LOG=info
